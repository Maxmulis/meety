<%= turbo_frame_tag "main" do %>
  <a href="/" class="form-button">Make another one</a>
  <div class="container">
    <div style="width: 100%; height: 600px; margin-top: 50px;"
      data-controller="mapbox"
      data-mapbox-people-markers-value="<%= @people_markers.to_json %>"
      data-mapbox-token-value="<%= @token %>"
      data-mapbox-categories-value="<%= @categories %>">
    </div>
  </div>
<% end %>
